{
  "name": "claude-code-dispatcher",
  "version": "1.0.0",
  "description": "CLI tool to integrate ClaudeCode with GitHub for automated issue processing",
  "main": "dist/index.js",
  "bin": {
    "claude-code-dispatcher": "./bin/claude-code-dispatcher"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/commands/index.js",
    "dev": "ts-node src/commands/index.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "claude",
    "github",
    "automation",
    "cli",
    "dispatcher"
  ],
  "author": "k1e1n04",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}